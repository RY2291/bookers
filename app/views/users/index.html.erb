<h1>Users Info</h1>
<%= render "info", user: @user %>
<h1>New Book</h1>
<%= render "form", book: @book %>
<h1>Users</h1>
<table>
  <thead>
    <tr>
      <td>image</td>
      <td>name</td>
      <td></td>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= attachment_image_tag user, :profile_image_id, :fill, 100, 100, fallback: "no_image.png" %></td>
      <td><%= user.name  %></td>
      <td><%= link_to "show", user_path(user.id) %></td>
    </tr>
  </tbody>
  <% end %>
</table>